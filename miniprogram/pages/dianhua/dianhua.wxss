/* pages/dianhua/dianhua.wxss */
.table {
  border: 0px solid darkgray;
  margin-bottom: 40rpx;
  border: 2rpx solid #eee;
  border-radius: 50rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 50%;
  justify-content: center;
  background:#1abc9c;
  color: #000;
  display: flex;
  height: 3rem;
  align-items: center;
  font-size: 48rpx;
}
.big{
  width: 94%;
  margin-left: 3%;
}
